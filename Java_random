		//methods to define array; method 1
		int[] a = {};
		//method 2
		int[] b;//declare array in one dimension
		b = new int[12];//assign ram to array
		Random rand1 = new Random();
		int d = rand1.nextInt(100);
		System.out.println(d);
		double e = rand1.nextDouble();
		System.out.println(e);
		boolean f = rand1.nextBoolean();
		System.out.println(f);
		float g = rand1.nextFloat();
		System.out.println(g);
		double h = rand1.nextGaussian();
		System.out.println(h);
		int i = rand1.nextInt(1);
		System.out.println(i);
		long j = rand1.nextLong();
		System.out.println(j);
	}

}

