class TestThread implements Runnable
{
	public void run()
	{
		for(int i = 0; i < 5; i++)
		{
			System.out.println("TestThread is running!");
			try {
				Thread.sleep(1000);
			}catch(InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}
public class MyThread2 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		TestThread t = new TestThread();
		new Thread(t).start();//用t是不行的，因为TestThread未能覆盖start方法，而Runnable接口是没有具体方法体的
		for(int i = 0; i < 5; i++)
		{
			System.out.println("MainThread is running!");
			try {
				Thread.sleep(1000);//用t也是不行的，因为TestThread未能覆盖sleep方法，而Runnable接口是没有具体方法体的
			}catch(InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}
